{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "allowImportingTsExtensions": true,
    "noEmit": true,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "preserveSymlinks": true,
    "baseUrl": ".",
    "paths": {
      "../client.ts": ["./client.ts"]
    }
  },
  "include": ["**/*.ts"],
  "exclude": ["node_modules", "generated"]
}
