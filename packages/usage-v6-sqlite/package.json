{
  "name": "@gcm/usage-v6-sqlite",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "gen": "prisma generate",
    "test": "pnpm test:ts && node --import ../../shared-tests/client-resolver.mjs --import ../../shared-tests/test-setup.mjs --test './tests/core/**/*.spec.ts' 'tests/readme/**/*.ts' 'tests/dialect/sqlite/**/*.ts'",
    "test:ts": "tsc --noEmit",
    "test:core": "node --import ../../shared-tests/client-resolver.mjs --import ../../shared-tests/test-setup.mjs --test './tests/core/**/*.spec.ts'",
    "test:readme": "node --import ../../shared-tests/client-resolver.mjs --import ../../shared-tests/test-setup.mjs --test './tests/readme/**/*.ts'",
    "test:dialect": "node --import ../../shared-tests/client-resolver.mjs --import ../../shared-tests/test-setup.mjs --test './tests/dialect/sqlite/**/*.ts'",
    "p:r": "prisma db push --force-reset && prisma db seed"
  },
  "imports": {
    "#client": "./src/client.ts",
    "#prisma-ts-select": "./generated/prisma-ts-select/*",
    "#dialect": "./generated/prisma-ts-select/dialects/index.js",
    "#extend": "./generated/prisma-ts-select/extend.js"
  },
  "dependencies": {
    "@gcm/prisma-ts-select": "workspace:*"
  },
  "devDependencies": {
    "@prisma/client": "6.19.2",
    "@types/node": "^24.0.0",
    "prisma": "6.19.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
